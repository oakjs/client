{
  "name": "oakjs",
  "version": "0.0.1",
  "description": "oakjs front end framwork",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/oakjs/oakjs"
  },
  "scripts": {
    "setup": "rm -rf node_modules && npm install && cd node_modules && ln -s ../src/oak . && ln -s ../src/oak-roots . && cd .. && echo 'Setup complete.  Start the server with: npm start'",
    "start": "rimraf build && ./node_modules/.bin/babel-node src/server/server",
    "test": "mocha --compilers js:babel/register -u bdd -R spec --timeout 500 --recursive",
    "build": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.production.config.babel.js --progress --profile --colors",
    "eslint": "eslint .",
    "jscs": "jscs .",
    "stats": "webpack --profile --json > stats.json",
    "prodstats": "set NODE_ENV=production && webpack --config ./webpack.production.config.babel.js --profile --json > prodstats.json"
  },
  "dependencies": {
    "he": "^1.1.0",
    "jquery": "^2.2.4",
    "lodash": "^4.5.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^0.9.0"
  },
  "devDependencies": {
    "acorn-jsx": "^3.0.1",
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^5.3.2",
    "body-parser": "^1.15.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "core-decorators": "^0.10.0",
    "css-loader": "^0.19.0",
    "enzyme": "^2.2.0",
    "es6-promise": "^3.0.2",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.26.5",
    "fs-promise": "^0.4.1",
    "html-webpack-plugin": "^1.6.1",
    "jsdom": "^6.5.1",
    "json-loader": "^0.5.3",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.2",
    "nock": "^8.1.0",
    "npm-install-webpack-plugin": "^4.0.4",
    "raw-loader": "^0.5.1",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-router": "^2.8.1",
    "redux-mock-store": "^1.2.1",
    "request": "^2.64.0",
    "rimraf": "^2.4.3",
    "semantic-ui-css": "^2.1.8",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.0",
    "webpack-hot-middleware": "^2.4.1",
    "webpack-validator": "^2.2.7"
  },
  "NOTES-babel": "The following is the config for babel 5.8 WITHOUT Hot Module Reload",
  "babel": {
    "stage": 1,
    "externalHelpers": true
  },
  "keywords": [
    "oak",
    "oakjs",
    "react"
  ],
  "author": "Owen Williams <owenoak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oakjs/client/issues"
  },
  "homepage": "https://github.com/oakjs/client"
}

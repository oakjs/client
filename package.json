{
  "name": "oakjs",
  "version": "0.0.1",
  "description": "oakjs front end framwork",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/oakjs/client"
  },
  "scripts": {
    "mocha-test": "mocha -u bdd -R spec --timeout 500 --recursive",
    "test": "cucumber.js --format pretty",
    "start": "./node_modules/.bin/babel-node src/server/server",
    "build": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "dependencies": {
    "acorn-jsx": "^2.0.1",
    "body-parser": "^1.15.0",
    "fs-extra": "^0.26.5",
    "fs-promise": "^0.4.1",
    "he": "^1.1.0",
    "history": "^1.12.3",
    "lodash": "^4.5.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc1",
    "whatwg-fetch": "^0.9.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-plugin-external-helpers-2": "^6.3.13",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "core-decorators": "^0.10.0",
    "css-loader": "^0.19.0",
    "cucumber": "^0.10.1",
    "enzyme": "^2.2.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^1.6.1",
    "jquery": "^2.2.0",
    "jsdom": "^6.5.1",
    "json-loader": "^0.5.3",
    "karma": "^0.13.22",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.2",
    "nodemon": "^1.7.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "request": "^2.64.0",
    "rimraf": "^2.4.3",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.4.0",
    "rollup-plugin-uglify": "^0.2.0",
    "semantic-ui-css": "^2.1.8",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "NOTES-babel": [
    "There are two babel configs below:  'babel' and 'babel-hmr'.",
    "To enable HMR, rename 'babel' to 'babel-no-hmr' and rename 'babel-hmr' to 'babel'."
  ],
  "babel": {
    "stage": 1
  },
  "babel-hmr": {
    "stage": 1,
    "env": {
      "development": {
        "plugins": [
          "react-transform"
        ],
        "extra": {
          "react-transform": {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": [
                  "react"
                ],
                "locals": [
                  "module"
                ]
              },
              {
                "transform": "react-transform-catch-errors",
                "imports": [
                  "react",
                  "redbox-react"
                ]
              }
            ]
          }
        }
      }
    }
  },
  "keywords": [
    "oak",
    "oakjs",
    "react"
  ],
  "author": "owenoak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oakjs/client/issues"
  },
  "homepage": "https://github.com/oakjs/client"
}

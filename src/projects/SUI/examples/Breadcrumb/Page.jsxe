    <Example title="Inline content" hint="<Breadcrumb>...</Breadcrumb>" oid="GmVe8RQI">
      <SUI.Breadcrumb oid="hv3Ww5l2">
        <a href="#" className="section" oid="DJO6BUm8">
          Home
        </a>
        <div className="divider" oid="f6vDqwxA">
          /
        </div>
        <div className="active section" oid="JNyrRWcB">
          Search for:
          <a href="#" oid="CtsaADKF">
            paper towels
          </a>
        </div>
      </SUI.Breadcrumb>
    </Example>
  </PageSection>
  <PageSection title="Appearance" oid="BMWcoKg2">
    <Example title="Text divider" hint="<Breadcrumb divider='/'/>" oid="dx5DqfN8">
      <SUI.Breadcrumb items={data.map} oid="NCfLcyTg"/>
      <Oak.Spacer oid="tNTBBuFt"/>
      <SUI.Breadcrumb divider="â€¢" items={data.map} oid="brS9bQmv"/>

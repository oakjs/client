    <Example title="Icon divider" hint="<Breadcrumb icon='right chevron'/>" oid="xxeFwpgm">
      <SUI.Breadcrumb icon="right chevron" items={data.map} oid="pgm45tBH"/>
      <Oak.Spacer oid="REtYiGfn"/>
      <SUI.Breadcrumb icon="right arrow" items={data.map} oid="x8eIQHyl"/>
    </Example>
    <Example title="Size" hint="<Breadcrumb size='small'/>" oid="UTjGIhuU">
      <SUI.Breadcrumb size="small" items={data.map} oid="reKcueHc"/>
      <Oak.Spacer oid="sShqMjhI"/>
      <SUI.Breadcrumb size="medium" items={data.map} oid="wPqLU2Ry"/>
      <Oak.Spacer oid="tVMLZJrv"/>
      <SUI.Breadcrumb size="large" items={data.map} oid="fVH7tsUq"/>
      <Oak.Spacer oid="DjO9vu22"/>
      <SUI.Breadcrumb size="huge" items={data.map} oid="bpG6BnCp"/>
    </Example>
  </PageSection>
</Oak.Page>

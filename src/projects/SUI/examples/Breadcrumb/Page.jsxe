<Oak.Page id="Breadcrumb" title="Breadcrumb" oid="KKwTjYmr">
  <PageTitle title="Breadcrumb" oid="uUT0BNJE">
    A breadcrumb is used to show hierarchy between content
  </PageTitle>
  <PageSection title="Content" oid="ZZd8qTC4">
    <Example title="Map of items" hint="<Breadcrumb items={{...}}/>" oid="auAbr1nl">
      <SUI.Breadcrumb items={data.map} oid="LFc5rl61"/>
    </Example>
    <Example title="List of item objects" hint="<Breadcrumb items={[ {link, title, active}, ...]}/>" oid="WVw4mIvO">
      <SUI.Breadcrumb items={data.items} oid="LcA9kqBq"/>
    </Example>
    <Example title="List of strings" hint="<Breadcrumb items={[ '...', '...', ...}/>" oid="LS0LMq1k">
      <SUI.Breadcrumb items={data.strings} oid="DF2ZHtn5"/>
